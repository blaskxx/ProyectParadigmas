options{  JDK_VERSION = "1.8";  static = true;}PARSER_BEGIN(EG1)package application;

public class EG1{  public static void main(String args []) throws ParseException  {    EG1 parser = new EG1(System.in);    while (true)    {      System.out.println("Reading  dfrom standard input...");     System.out.print("Enter an expression like \"CREATE OBJECT NOMBRE(ATRIBUTO TIPO);\" :");      try      {        switch (EG1.one_line())        {          case 0 :           System.out.println("OK..");          break;          case 1 :           System.out.println("Goodbye.");          break;          default :           break;        }      }      catch (Exception e)      {        System.out.println("NOK.");        System.out.println(e.getMessage());        EG1.ReInit(System.in);      }      catch (Error e)      {        System.out.println("Oops.");        System.out.println(e.getMessage());        break;      }    }  }}PARSER_END(EG1)SKIP :{  " "| "\r"| "\t"| "\n"}TOKEN : /* PALABRAS RESERVADAS */{  < CREATE : "CREATE" >| < OBJECT : "OBJECT" >| < SIMBOLO_INIT : "(" >| < SIMBOLO_FIN : ");" >}TOKEN :{  < CONSTANT : (< PALABRA >)+ >| < #PALABRA : [ "a"-"z" ] >}int one_line() :{}{  sum() ";"  {    return 0;  }| ";"  {    return 1;  }}void sum() :{}{  term()  (    (      < CREATE >    | < OBJECT >    )    term()  )*}void term() :{}{  unary()  (    (      < SIMBOLO_INIT >    | < SIMBOLO_FIN >    )    unary()  )*}void unary() :{}{  < OBJECT > element()| element()}void element() :{}{  < CONSTANT >| "(" sum() ")"}
